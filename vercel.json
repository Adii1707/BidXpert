{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "/api/index.js" },
        { "src": "/(.*)", "dest": "index.js" }
    ],
    "functions": {
      "api/index.js": {
        "maxDuration": 10
      }
    }
  }
  